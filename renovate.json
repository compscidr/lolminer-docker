{
    "customManagers": [
        {
            "customType": "regex",
            "fileMatch": [
                "Dockerfile"
            ],
            "matchStrings": [
                "ARG LOLMINER_VERSION=[\"']?(?<currentValue>.+?)[\"']?\\s+"
            ],
            "datasourceTemplate": "githbub-releases",
            "depNameTemplate": "Lolliedieb/lolMiner-releases"
        }
    ]
}
